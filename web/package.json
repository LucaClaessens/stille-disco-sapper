    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@movingbrands/svelte-portable-text": "^0.0.11",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-json": "^4.0.0",
